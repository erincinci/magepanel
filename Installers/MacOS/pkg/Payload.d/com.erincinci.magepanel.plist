<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
  <dict>
    <key>KeepAlive</key>
    <dict>
      <key>SuccessfulExit</key>
      <true/>
    </dict>
    <key>Label</key> 
    <string>com.erincinci.magepanel</string>
    <key>ProgramArguments</key>
    <array>
	  <string>sh</string>
	  <string>/opt/MagePanel/init-server-mac.sh</string>
    </array>
    <key>UserName</key>
    <string>root</string>
    <key>RunAtLoad</key>
    <true/>
    <key>KeepAlive</key>
    <true/>
    <key>OnDemand</key>
    <false/>
    <key>StandardOutPath</key>
    <string>/var/log/magepanel.info.log</string>
    <key>StandardErrorPath</key>
    <string>/var/log/magepanel.err.log</string>
    <key>Debug</key>
    <true/>
  </dict>
</plist>

{
  "name": "magepanel",
  "description": "UI for MagePHP Deployment Tool",
  "homepage": "http://erincinci.github.io/magepanel/",
  "license": "LGPL 2.1",
  "keywords": [
    "Magallanes",
    "MagePHP",
    "deployment",
    "UI"
  ],
  "author": "Erinç İnci <erincinci@gmail.com> (https://about.me/erincinci)",
  "contributors": [
    "Murat Aksoy"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/erincinci/magepanel.git"
  },
  "main": "app.js",
  "version": "1.8.0",
  "scripts": {
    "prestart": "npm update",
    "start": "pm2 start ./node_modules/nodemon/bin/nodemon.js --name MagePanel -- app.js",
    "stop": "pm2 stop MagePanel",
    "restart": "pm2 stop MagePanel; npm update; pm2 start MagePanel",
    "test": "node ./tests/",
    "install-windows-service": "winser -i",
    "uninstall-windows-service": "winser -r"
  },
  "dependencies": {
    "ansi-to-html": "0.4.1",
    "cli-color": "0.3.2",
    "connect": "3.4.1",
    "cron": "1.1.0",
    "express.io": "1.1.13",
    "fs-extra": "0.26.5",
    "git-repo-info": "1.1.2",
    "graceful-fs": "4.1.3",
    "jade": "1.11.0",
    "mkdirp": "0.5.1",
    "moment": "2.11.2",
    "node-uuid": "1.4.7",
    "nodejs-model": "0.1.6",
    "nodemailer": "2.2.1",
    "nodemailer-mandrill-transport": "1.1.0",
    "nodemailer-sendgrid-transport": "0.2.0",
    "nodemailer-sendmail-transport": "1.0.0",
    "nodemailer-ses-transport": "1.3.0",
    "nodemailer-smtp-transport": "2.4.1",
    "nodemon": "1.3.5",
    "nstore": "0.5.2",
    "path": "0.12.7",
    "rimraf": "2.5.2",
    "rollbar": "0.5.13",
    "scribe-js": "2.0.4",
    "string": "3.3.1",
    "stylus": "0.53.0",
    "underscore": "1.8.3",
    "user-settings": "0.1.5",
    "username": "2.1.0",
    "validator": "5.1.0",
    "winston": "1.0.2"
  },
